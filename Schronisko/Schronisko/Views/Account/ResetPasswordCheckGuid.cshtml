
@{
    ViewBag.Title = "ResetPasswordCheckGuid";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
<div class="row">

    <div class="form-group">
        @Html.Label("Kod do resetowania hasła", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBox("guid",null, new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
   
    @Html.Label("Adres email") @Html.TextBox("email")
    @Html.Label("Nowe hasło") @Html.Password("password")
</div>
}

